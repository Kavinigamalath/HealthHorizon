      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.22"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.475" />

  <Button
      android:id="@+id/button91"
      android:layout_width="139dp"
      android:layout_height="54dp"
      android:backgroundTint="@color/primaryColor"
      android:text="Cycling"
      android:textColor="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.808"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.475" />

  <LinearLayout
      android:layout_width="330dp"
      android:layout_height="182dp"
      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="@+id/imageView14"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.506"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/view"
      app:layout_constraintVertical_bias="0.316">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="156dp"
        android:layout_height="158dp"
        android:background="#E7E5E5"
        app:srcCompat="@drawable/img_17" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="156dp"
        android:layout_height="158dp"
        app:srcCompat="@drawable/img_16" />
  </LinearLayout>

  <LinearLayout
      android:id="@+id/linearLayout2"
      android:layout_width="337dp"
      android:layout_height="180dp"
      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.539"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.717">

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="153dp"
        android:layout_height="155dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/img_20"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="451dp" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="153dp"
        android:layout_height="155dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/img_19"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="451dp" />

  </LinearLayout>

  <Button
      android:id="@+id/button92"
      android:layout_width="139dp"
      android:layout_height="54dp"
      android:backgroundTint="@color/primaryColor"
      android:text="Yoga"
      android:textColor="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.221"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.833" />

  <Button
      android:id="@+id/button93"
      android:layout_width="139dp"
      android:layout_height="54dp"
      android:backgroundTint="@color/primaryColor"
      android:text="exercising"
      android:textColor="@color/black"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.83"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.833" />

  <ImageView
      android:id="@+id/imageView24"
      android:layout_width="88dp"
      android:layout_height="57dp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView16"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.876"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.036"
      app:srcCompat="@drawable/img_21" />

  <ImageView
      android:id="@+id/imageView15"
      android:layout_width="56dp"
      android:layout_height="38dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.011"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.035"
      app:srcCompat="@drawable/img_23" />

</androidx.constraintlayout.widget.ConstraintLayout>